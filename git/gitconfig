[alias]
    br = branch -avv
    change = checkout
    fee = fetch --prune --all
    logline = log --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit
    oops = reset HEAD~1
    ooops = oops
    pu = push origin HEAD
    re = remote -v
    settings = config --list --show-origin
    subpull = submodule update --remote --init
    what = log -n 5 --stat

[include]
    path = .git-identity