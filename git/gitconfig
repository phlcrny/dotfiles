[alias]
    add-executable = add --chmod=+x
    al = commit --amend
    br = branch -avv
    co = checkout .
    change = checkout
    fee = fetch --prune --all
    find = log --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit -i --grep
    last-commit = log -n 1 --stat
    last-diff = diff HEAD~1..HEAD
    logline = log -n 10 --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit
    ls = ls-files --full-name --eol --stage
    ls-excluded = ls-files --full-name --eol -o
    oops = reset HEAD~1
    pu = push origin HEAD
    push-no-ci = push -o ci.skip
    re = remote -v
    remove = branch -D
    search = log --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit -S
    settings = config --list --show-origin
    squash = merge --squash --no-commit
    subpull = submodule update --remote --init
    what = log -n 5 --stat
    # aliases of aliases
    amend-last = al
    search-content = search
    search-msg = find
    find-by-msg = find
    find-by-content = search
    lc = last-commit
    ld = last-diff
    ll = logline
    ooops = oops
    remoev = remove
[include]
    path = .git_extras
[init]
    defaultBranch = main
[rerere]
    enabled = true
    autoupdate = true
