[alias]
    add-executable = add --chmod=+x
    br = branch -avv
    co = checkout .
    change = checkout
    fee = fetch --prune --all
    find = log --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit -i --grep
    last-commit = log -n 1 --stat
    last-diff = diff HEAD~1..HEAD
    logline = log -n 10 --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit
    oops = reset HEAD~1
    pu = push origin HEAD
    re = remote -v
    remove = branch -D
    settings = config --list --show-origin
    squash = merge --squash --no-commit
    subpull = submodule update --remote --init
    what = log -n 5 --stat
    # aliases of aliases
    lc = last-commit
    ld = last-diff
    ll = logline
    ooops = oops
[include]
    path = .git-extras
[init]
	defaultBranch = main
