[alias]
    add-executable = add --chmod=+x
    br = branch -avv
    change = checkout
    fee = fetch --prune --all
    logline = log -n 10 --graph --pretty=format:'%Cred%cr%Creset - %Cgreen%h%Creset: %s %d%C(cyan)<%aN/%ae>%Creset' --abbrev-commit
    oops = reset HEAD~1
    ooops = oops
    pu = push origin HEAD
    re = remote -v
    remove = branch -D
    settings = config --list --show-origin
    squash = merge --squash
    subpull = submodule update --remote --init
    what = log -n 5 --stat
[include]
    path = .git-identity
    path = .git-diff-so-fancy
